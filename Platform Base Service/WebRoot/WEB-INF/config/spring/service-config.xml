	</bean>
	<jaxws:endpoint id="ep_DicdataRelationWebService"
		implementorClass="com.asc.bs.webservice.service.DicdataRelationWebService"
		implementor="#DicdataRelationWebService"
		address="/DicdataRelationWebService" />
	
	<bean id="OrganizationWebService" class="com.asc.bs.webservice.service.OrganizationWebService">
		<property name="reader">
			<ref bean="OrganizationReaderImp" />
		</property>
		<property name="writer">
			<ref bean="OrganizationWriterImp" />
		</property>
	</bean>
	<jaxws:endpoint id="ep_OrganizationWebService"
		implementorClass="com.asc.bs.webservice.service.OrganizationWebService"
		implementor="#OrganizationWebService"
		address="/OrganizationWebService" />
	
	<bean id="ModuleRoleMappingWebService" class="com.asc.bs.webservice.service.ModuleRoleMappingWebService">
		<property name="reader">
			<ref bean="DbacModuleRoleMappingAccessor" />
		</property>
	</bean>
	<jaxws:endpoint id="ep_ModuleRoleMappingWebService"
		implementorClass="com.asc.bs.webservice.service.ModuleRoleMappingWebService"
		implementor="#ModuleRoleMappingWebService"
		address="/ModuleRoleMappingWebService" />
	
	<bean id="WorkFlowRoleMappingWebService" class="com.asc.bs.webservice.service.WorkFlowRoleMappingWebService">
		<property name="reader">
