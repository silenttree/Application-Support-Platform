	xmlns:jaxws="http://cxf.apache.org/jaxws">
	
	<bean id="ApplicationWebService" class="com.asc.bs.webservice.service.ApplicationWebService">
		<property name="reader">
			<ref bean="DbacApplicationReaderImp" />
		</property>
	</bean>
	<jaxws:endpoint id="ep_ApplicationWebService"
		implementorClass="com.asc.bs.webservice.service.ApplicationWebService"
		implementor="#ApplicationWebService"
		address="/ApplicationWebService" />
	
	<bean id="AppEntWebService" class="com.asc.bs.webservice.service.AppEntWebService">
		<property name="reader">
			<ref bean="DbacApplicationEntranceReader" />
		</property>
	</bean>
	<jaxws:endpoint id="ep_AppEntWebService"
		implementorClass="com.asc.bs.webservice.service.AppEntWebService"
		implementor="#AppEntWebService"
		address="/AppEntWebService" />
	
	<bean id="DictionaryWebService" class="com.asc.bs.webservice.service.DictionaryWebService">
		<property name="reader">
			<ref bean="DictionaryReaderImp" />
		</property>
	</bean>
	<jaxws:endpoint id="ep_DictionaryWebService"
		implementorClass="com.asc.bs.webservice.service.DictionaryWebService"
		implementor="#DictionaryWebService"
		address="/DictionaryWebService" />
	
	<bean id="DicdataRelationWebService" class="com.asc.bs.webservice.service.DicdataRelationWebService">
		<property name="reader">
			<ref bean="DicdataRelationReaderImp" />
