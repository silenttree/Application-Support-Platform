	<jaxws:endpoint id="ep_WorkFlowRoleMappingWebService"
		implementorClass="com.asc.bs.webservice.service.WorkFlowRoleMappingWebService"
		implementor="#WorkFlowRoleMappingWebService"
		address="/WorkFlowRoleMappingWebService" />
	
	<bean id="LogConfigWebService" class="com.asc.bs.webservice.service.LogConfigWebService">
		<property name="reader">
			<ref bean="LogConfigReaderImpl" />
		</property>
	</bean>
	<jaxws:endpoint id="ep_LogConfigWebService"
		implementorClass="com.asc.bs.webservice.service.LogConfigWebService"
		implementor="#LogConfigWebService"
		address="/LogConfigWebService" />
		
	<bean id="DataFilterSettingsWebService" class="com.asc.bs.webservice.service.DataFilterSettingsWebService">
		<property name="readerImpl">
			<ref bean="DataFilterSettingsReaderImpl"/>
		</property>
	</bean>
	<bean id="DataFilterSettingsReaderImpl" class="com.asc.bs.datafilter.access.imp.DataFilterSettingsReaderImpl"/>
	
	<jaxws:endpoint id="ep_DataFilterSettingsWebService"
		implementorClass="com.asc.bs.webservice.service.DataFilterSettingsWebService"
		implementor="#DataFilterSettingsWebService"
		address="/DataFilterSettingsWebService" />
		
	<bean id="DocWebService" class="com.asc.bs.webservice.service.DocWebService">
		<property name="reader">
			<ref bean="DocReaderImp" />
		</property>
	</bean>
	<jaxws:endpoint id="ep_DocWebService"
		implementor="#DocWebService"
		address="/DocWebService" />
		
		
